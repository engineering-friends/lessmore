{
  "title": "cmd-tab to cmd-option-tab",
  "rules": [
    {
      "description": "cmd-tab to cmd-shift-tab",
      "manipulators": [
        {
          "from": {
            "key_code": "tab",
            "modifiers": {
              "mandatory": [
                "left_command"
              ],
              "optional": ["any"]
            }

          },
              "to": [
                {
                  "key_code": "tab",
                  "modifiers": [
                    "left_command",
                    "left_option"
                  ]
                }
              ],
          "type": "basic"
        }
        ]
    }
  ]
}
