{
    "manifest_version": 2,
    "name": "Safari Bookmarks Highlighter",
    "version": "1.0",
    "description": "A simple Safari extension",
    "icons": {
        "48": "images/icon-48.png",
        "96": "images/icon-96.png"
    },
    "browser_action": {
        "default_popup": "popup.html",
        "default_icon": {
            "48": "images/icon-48.png",
            "96": "images/icon-96.png"
        }
    }
}